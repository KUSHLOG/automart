{
  "name": "automart",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "dev:chrome": "concurrently \"npm run dev\" \"npm run open:chrome\"",
    "dev:safari": "concurrently \"npm run dev\" \"npm run open:safari\"",
    "dev:browser": "concurrently \"npm run dev\" \"npm run open:browser\"",
    "open:chrome": "sleep 3 && open -a 'Google Chrome' http://localhost:3000",
    "open:safari": "sleep 3 && open -a 'Safari' http://localhost:3000",
    "open:browser": "sleep 3 && open http://localhost:3000",
    "start:chrome": "concurrently \"npm run start\" \"npm run open:chrome\"",
    "start:safari": "concurrently \"npm run start\" \"npm run open:safari\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "format": "npm run prettier",
    "type-check": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "node scripts/test-e2e.js",
    "test:e2e:raw": "playwright test",
    "test:e2e:bidding": "node scripts/test-e2e.js tests/e2e/bidding.spec.ts",
    "test:e2e:auth": "node scripts/test-e2e.js tests/e2e/auth.spec.ts",
    "test:all": "npm run lint && npm run prettier:check && npm run type-check && npm run test && npm run test:e2e",
    "test:ci": "npm run lint && npm run prettier:check && npm run type-check && npm run test",
    "seed": "tsx prisma/seed.ts",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "clean": "rm -rf .next node_modules/.cache",
    "setup": "npm run db:generate && npm run seed"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "bcrypt": "^6.0.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "next": "15.5.2",
    "next-auth": "^5.0.0-beta.25",
    "prisma": "^6.15.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcrypt": "^6.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.3",
    "axe-core": "^4.10.3",
    "axe-playwright": "^2.1.0",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.1.2",
    "jest-environment-jsdom": "^30.1.2",
    "lighthouse": "^12.8.2",
    "msw": "^2.11.1",
    "playwright-lighthouse": "^4.0.0",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "typescript": "^5",
    "web-streams-polyfill": "^4.2.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
