{
  "name": "automart",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "dev:chrome": "./scripts/dev-browser.sh chrome",
    "dev:safari": "./scripts/dev-browser.sh safari",
    "dev:browser": "./scripts/dev-browser.sh",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:ci": "npm run lint && npm run prettier:check && npm run type-check && npm run test",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:seed": "tsx prisma/seed.ts",
    "db:setup": "npm run db:generate && npm run db:seed",
    "clean": "rm -rf .next node_modules/.cache"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "@tailwindcss/postcss": "^4.1.13",
    "bcrypt": "^6.0.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "next": "15.5.2",
    "next-auth": "^5.0.0-beta.25",
    "prisma": "^6.15.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcrypt": "^6.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.1.2",
    "jest-environment-jsdom": "^30.1.2",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
